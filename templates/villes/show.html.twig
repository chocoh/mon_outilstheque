{% extends 'base.html.twig' %}

{% block title %}Villes{% endblock %}

{% block body %}
    <h1>Villes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ville.id }}</td>
            </tr>
            <tr>
                <th>Departement_code</th>
                <td>{{ ville.departementCode }}</td>
            </tr>
            <tr>
                <th>Insee_code</th>
                <td>{{ ville.inseeCode }}</td>
            </tr>
            <tr>
                <th>Zip_code</th>
                <td>{{ ville.zipCode }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ville.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ ville.slug }}</td>
            </tr>
            <tr>
                <th>Gps_lat</th>
                <td>{{ ville.gpsLat }}</td>
            </tr>
            <tr>
                <th>Gps_Ing</th>
                <td>{{ ville.gpsIng }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('villes_index') }}">back to list</a>

    <a href="{{ path('villes_edit', {'id': ville.id}) }}">edit</a>

    {{ include('villes/_delete_form.html.twig') }}
{% endblock %}
